{
  "expo": {
    "name": "Healthy Corners Rewards",
    "slug": "healthy-corners-rewards",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "primaryColor": "#008550",
    "orientation": "portrait",
    "icon": "./assets/images/CustomerAppIcon.png",
    "splash": {
      "image": "./assets/images/DCHC_Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "org.calblueprint.HealthyCornersRewards",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to help people locate nearby stores to find sources for healthy produce."
      }
    },
    "android": {
      "package": "org.calblueprint.HealthyCornersRewards",
      "googleServicesFile": "./google-services.json",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}