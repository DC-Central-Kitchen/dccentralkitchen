{
  "expo": {
    "name": "Healthy Corners",
    "slug": "healthy-corners",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "plugins": [
      "./react-native-maps-plugin",
        "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "android": {
          "compileSdkVersion": 33,
          "targetSdkVersion": 33,
          "buildToolsVersion": "33.0.0"
        },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "version": "1.6.38",
    "primaryColor": "#008550",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/hc_splash_small.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "healthycorners",
    "updates": {
      "fallbackToCacheTimeout": 3000
    },
    "assetBundlePatterns": ["assets/images/*"],
    "ios": {
      "bundleIdentifier": "org.calblueprint.HealthyCornersRewards",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "1.6.21",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "To find nearby stores and see what products are available.",
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "waze"
        ],
        "ITSAppUsesNonExemptEncryption": "NO"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAt2wwpTvYIBhVpgkFZ3fa7dY3Kw2cc5uU",
        "branch": {
          "apiKey": "key_live_ikGZzufqsAKf84CYNZ1fGpkcBtlLsdc7"
        }
      }
    },
    "android": {
      "package": "org.calblueprint.HealthyCornersRewards",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDlGN8NprlbMezFbPt_emrGgTcadW8MPio"
        },
        "branch": {
          "apiKey": "key_live_ikGZzufqsAKf84CYNZ1fGpkcBtlLsdc7"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "VIBRATE"
      ],
      "versionCode": 38
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "dca9a0a2-4c07-4f8d-98fc-a38b8cdfca70"
      }
    }
  }
}
